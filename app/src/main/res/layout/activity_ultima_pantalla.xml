<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jesus.practicaandroid.UltimaPantalla">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/lblUsuario"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="@string/lblUsuFinal"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/lblCorrec"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/lblCorrec" />

        <TextView
            android:id="@+id/lblIncorrec"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/lblIncorrec" />

        <Button
            android:id="@+id/btnVolver"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/btnVolver" />

        <Button
            android:id="@+id/btnUrl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="pulsaSoluciones"
            android:text="@string/btnUrl" />
    </LinearLayout>
</android.support.constraint.ConstraintLayout>
